# T-mobile API Configuration
# Copy this file to .env and update with your actual T-mobile API credentials

# T-mobile Carrier Billing API
TMOBILE_API_BASE_URL=https://api.t-mobile.com/billing/v1
TMOBILE_API_KEY=your_tmobile_api_key_here
TMOBILE_MERCHANT_ID=your_merchant_id_here

# Optional: Development/Testing settings
TMOBILE_SANDBOX_MODE=true
TMOBILE_TEST_PHONE_NUMBER=505-123-4567